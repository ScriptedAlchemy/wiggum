{
  "name": "@wiggum/rsbuild-plugin-wiggum",
  "version": "0.1.0",
  "description": "Rsbuild plugin for adding a floating chat widget to web applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "prompts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./src/widget-entry": "./src/widget-entry.tsx"
  },
  "scripts": {
    "build": "rslib build",
    "build:watch": "rslib build --watch",
    "dev": "rslib build --watch",
    "prepublishOnly": "npm run build",
    "publint": "publint"
  },
  "keywords": [
    "rsbuild",
    "plugin",
    "chat",
    "widget",
    "react",
    "floating"
  ],
  "author": "Wiggum Team",
  "license": "MIT",
  "peerDependencies": {
    "@rsbuild/core": ">=1.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.7.3",
    "@rsbuild/plugin-react": "^1.4.5",
    "@rslib/core": "^0.19.6",
    "@types/node": "^25.2.3",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "publint": "^0.3.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@opencode-ai/sdk": "^0.6.10",
    "@wiggum/agent": "workspace:*",
    "clsx": "^2.1.1",
    "http-proxy-middleware": "^2.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wiggum/wiggum.git",
    "directory": "packages/rsbuild-plugin-wiggum"
  },
  "publishConfig": {
    "access": "public"
  }
}
