{
  "name": "@wiggum/cli",
  "version": "1.0.0",
  "type": "module",
  "description": "A CLI tool built with yargs",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "wiggum": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "start": "node bin/cli.js",
    "dev": "tsc --watch",
    "build": "tsc",
    "prebuild": "npm run clean",
    "test": "npm run build && jest",
    "clean": "rm -rf dist",
    "list:providers": "tsx list-opencode-providers.ts"
  },
  "keywords": [
    "cli",
    "yargs",
    "command-line"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@opencode-ai/sdk": "^0.6.10",
    "@wiggum/agent": "workspace:*",
    "chalk": "^4.1.2",
    "execa": "^8.0.1",
    "ora": "^5.4.1",
    "package-manager-detector": "^0.2.11",
    "which": "^4.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^20.19.12",
    "@types/which": "^3.0.4",
    "@types/yargs": "^17.0.33",
    "jest": "^29.7.0",
    "tsx": "^4.15.7",
    "rspress": "^1.45.3",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
